      {
        "key": "doors",
        "type": "String",
        "required": false,
        "description": "Number of doors"
      },
      {
        "key": "interiorColor",
        "type": "String",
        "required": false,
        "description": "List of vehicle interior colors"
      },
      {
        "key": "exteriorColor",
