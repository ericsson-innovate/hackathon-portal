        "key": "transmissionType",
        "type": "String",
        "required": false,
        "description": "Vehicle transmission type"
      },
      {
        "key": "weight",
        "type": "Float",
        "required": false,
        "description": "Vehicle weight (in lbs)"
      },
      {
        "key": "category",
        "type": "String",
        "required": false,
        "description": "Sedan, SUV, ..."
      },
      {
        "key": "options",
        "type": "String",
        "required": false,
        "description": "Vehicle options"
